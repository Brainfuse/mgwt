<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0.3//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.0.3/distro-source/core/src/gwt-module.dtd">
<module rename-to="showcase">

  

	<inherits name="com.google.gwt.user.User" />
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="de.kurka.gwt.mobile.MobileGwt"/>
	<inherits name="de.kurka.gwt.mobile.theme.iphone.IphoneTheme"/>
	
	<set-property name="user.agent" value="safari" />

	<entry-point class="de.kurka.mobile.showcase.client.ShowCaseEntryPoint" />
	
	<source path="client"/>
</module>