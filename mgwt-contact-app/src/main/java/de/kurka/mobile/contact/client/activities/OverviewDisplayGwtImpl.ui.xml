	<mgwt:LayoutPanel>
		<mgwt:HeaderPanel>
		
			<mgwt:center><g:HTML>Groups</g:HTML></mgwt:center>
			<mgwt:right><mgwt:HeaderRoundButton text="+" ui:field="plusButton"></mgwt:HeaderRoundButton></mgwt:right>
		</mgwt:HeaderPanel>
		<mgwt:ScrollPanel>
			<mgwt:child>
				<mgwt:CellList ui:field="list"/>
